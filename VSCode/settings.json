{
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "folders": [],
    "javascript.format.semicolons": "true",
    "javascript.preferences.quoteStyle": "single",
    "prettier.disableLanguages": [
        "markdown"
    ],
    "prettier.jsxSingleQuote": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "settings": {},
    "typescript.format.semicolons": "true",
    "typescript.preferences.quoteStyle": "single",
    "workbench.colorTheme": "Community Material Theme",
    "workbench.editor.enablePreview": false,
    "workbench.preferredDarkColorTheme": "Solarized Dark",
    "workbench.preferredHighContrastColorTheme": "Solarized Dark",
    "workbench.preferredLightColorTheme": "Quiet Light",
    "workbench.startupEditor": "newUntitledFile",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDelete": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": false
    },
    "files.exclude": {
        "**/*.swp": true,
        "**/node_modules": true
    },
    "eslint.format.enable": true,
    "editor.largeFileOptimizations": false,
    "window.zoomLevel": 0,
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "explorer.confirmDragAndDrop": false
}
